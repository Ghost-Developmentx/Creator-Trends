{
  "name": "user-service",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-docs && npm run verify-build",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "copy-docs": "node copy-docs.js",
    "start": "node -r module-alias/register dist/server.js",
    "deploy": "./deploy.sh",
    "clean": "rimraf dist && mkdir dist",
    "verify-build": "node verifyBuild.js",
    "switch:docker": "cross-env node switch-env.js docker",
    "switch:test": "cross-env node switch-env.js test",
    "switch:production": "cross-env node switch-env.js production",
    "start:docker": "npm run switch:docker && docker-compose up -d --build",
    "start:production": "npm run switch:production && npm run build && node -r module-alias/register dist/server.js",
    "migrate:docker": "npm run switch:docker && docker exec -it user-service-user-service-1 npx sequelize-cli db:migrate",
    "migrate:test": "npm run switch:test && sequelize-cli db:migrate",
    "migrate:local": "cross-env NODE_ENV=development DB_PORT=5433 sequelize-cli db:migrate",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-prom-bundle": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.2",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.8",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/js-yaml": "^4.0.9",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "_moduleAliases": {
    "@": "dist",
    "@controllers": "dist/controllers",
    "@models": "dist/models",
    "@config": "dist/config",
    "@services": "dist/services",
    "@utils": "dist/utils"
  }
}